<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"><!--mylessonssson listview的自定义布局-->
    <com.example.UI.ItemView
        android:id="@+id/lesson_name"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:left_icon_drawable="@drawable/ic_import_contacts_black_24dp"
        app:right_arrow_drawable="@drawable/ic_check_black_24dp"
        app:right_arrow_visible="false"/>
    <View
        android:id="@+id/view_underresource"
        android:layout_width="fill_parent"
        android:layout_height="0.5dp"
        android:background="#A9A9A9" />
</LinearLayout>
